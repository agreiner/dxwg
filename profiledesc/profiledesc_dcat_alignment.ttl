# baseURI: https://w3c.github.io/dxwg/profiledesc_dcat_alignment
# imports: http://www.w3.org/ns/dcat
# imports: https://w3c.github.io/dxwg/profiledesc

@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prof: <https://w3c.github.io/dxwg/profiledesc#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

dcterms:conformsTo
  rdf:type owl:ObjectProperty ;
  rdfs:comment "Alignment presumes that DCAT will also use this property to qualify profile conformance for distributions."@en ;
  rdfs:range dcterms:Standard ;
.

prof:ImplResourceDesc
  rdf:type owl:Class ;
  rdfs:comment "A resource that defines implementing rules for a profile. The language (or profile) that this is defined in is in turn specified using dct:conformsTo.  Must indicate the purpose (dct:type) and the formalism (dct:format) used to allow machine mediation of such rules."@en ;
  rdfs:label "Implementation Resource Descriptor"@en ;
  rdfs:comment "Or a superclass of dcat:Distribution if dcat:Distribution domain remains dcat:Dataset"@en ;
  rdfs:subClassOf dcat:Distribution ;
.

prof:Profile
  rdf:type owl:Class ;
  rdfs:comment "This is a generic association"@en ;
  rdfs:subClassOf dcat:Resource ;
.

prof:resource
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf dcat:distribution ;
.


<https://w3c.github.io/dxwg/profiledesc_dcat_alignment>
  rdf:type owl:Ontology ;
  dc:contributor "Nicholas Car" ;
  dc:creator "Rob Atkinson" ;
  dc:description "This ontology defines an alignment with DCAT through subclassing relevant DCAT types and properties, without limiting the scope of the main profileDesc ontology to DCAT."@en ;
  dcterms:contributor <http://orcid.org/0000-0002-8742-7730> ;
  rdfs:label "Profile Description alignment to DCAT concepts"@en ;
  owl:imports <http://www.w3.org/ns/dcat> ;
  owl:imports <https://w3c.github.io/dxwg/profiledesc> ;
  owl:versionIRI <http://promsns.org/def/prof/1.0> ;
  owl:versionInfo "1.0" ;
.
